{
  "name": "nextjs_playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3333",
    "build": "next build",
    "start": "next start",
    "test": "TZ=EST jest",
    "test-watch": "TZ=EST jest --watch",
    "test:ci": "jest --config ./jest.config.js --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --watchAll=false",
    "lint": "eslint --fix --ext=ts,tsx .",
    "download-schema": "apollo client:download-schema --endpoint=https://api.spacex.land/graphql/ schema.json",
    "update-types": "apollo codegen:generate --localSchemaFile=schema.json --target=typescript --includes='api/**/*.ts' --excludes='api/chat/**/*.ts' --tagName=gql --addTypename --globalTypesFile=api/graphql-global-types.ts types",
    "update-apollo-types": "yarn download-schema && yarn update-types"
  },
  "dependencies": {
    "@apollo/client": "^3.7.4",
    "@types/antd": "^1.0.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "antd": "^5.1.5",
    "apollo-upload-client": "^17.0.0",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "eslint-config-next": "13.1.2",
    "eslint-plugin-graphql": "^4.0.0",
    "graphql": "^16.6.0",
    "next": "13.1.2",
    "next-with-apollo": "^5.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.57.1",
    "tslint-plugin-graphql": "^0.0.6",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.1.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "babel-jest": "^29.3.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-junit": "^15.0.0",
    "prettier": "^2.8.3"
  }
}
